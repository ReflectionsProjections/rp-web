{
  "name": "@rp/site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:dependencies": "yarn workspaces foreach -Rt --from @rp/site --exclude @rp/site run build",
    "build": "yarn build:dependencies && vite build",
    "dev": "yarn build:dependencies && vite",
