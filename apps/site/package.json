    "verify": "yarn build && tsc --noEmit && yarn lint"
  },
  "dependencies": {
    "@chakra-ui/icons": "*",
    "@chakra-ui/react": "*",
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@rp/shared": "*",
    "framer-motion": "*",
    "moment": "^2.30.1",
    "react": "*",
    "react-audio-visualize": "^1.2.0",
    "react-dom": "*",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-autocomplete": "*",
    "@types/react-dom": "*",
    "@vitejs/plugin-react": "*",
    "@vitejs/plugin-react-swc": "*",
    "eslint": "*",
    "eslint-plugin-react-hooks": "*",
    "eslint-plugin-react-refresh": "*",
    "globals": "*",
    "prettier": "*",
    "typescript": "*",
    "typescript-eslint": "*",
    "vite": "*"
